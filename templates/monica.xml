      <ContainerDir>/var/www/monica/storage</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>production</Value>
      <Name>APP_ENV</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>false</Value>
      <Name>APP_DEBUG</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>ChangeMeBy32KeyLengthOrGenerated</Value>
      <Name>APP_KEY</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>ChangeMeBy20+KeyLength</Value>
      <Name>HASH_SALT</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>18</Value>
      <Name>HASH_LENGTH</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>APP_URL</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>mysql</Value>
      <Name>DB_CONNECTION</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>DB_HOST</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>3306</Value>
      <Name>DB_PORT</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>monica</Value>
      <Name>DB_DATABASE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>monica</Value>
      <Name>DB_USERNAME</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>monica</Value>
      <Name>DB_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>true</Value>
      <Name>DB_USE_UTF8MB4</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>smtp</Value>
      <Name>MAIL_DRIVER</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>smtp.example.com</Value>
      <Name>MAIL_HOST</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>587</Value>
      <Name>MAIL_PORT</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>TLS</Value>
      <Name>MAIL_ENCRYPTION</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>MAIL_USERNAME</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>MAIL_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>MAIL_FROM_ADDRESS</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>Monica instance</Value>
      <Name>MAIL_FROM_NAME</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>APP_EMAIL_NEW_USERS_NOTIFICATION</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>en</Value>
      <Name>APP_DEFAULT_LOCALE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>false</Value>
      <Name>APP_DISABLE_SIGNUP</Name>
      <Mode/>
    </Variable>
